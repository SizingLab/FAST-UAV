<FASTOAD_model>
  <specifications>
    <MTOW units="kg" is_input="True">100.0</MTOW>
    <altitude units="m" is_input="True">0.0</altitude>
    <dISA units="K" is_input="True">0.0</dISA>
    <distance>
      <climb units="m" is_input="True">0.0</climb>
      <forward units="m" is_input="True">17400.0</forward>
    </distance>
    <maxthrust>
      <k is_input="True">2.5</k>
    </maxthrust>
    <speed>
      <climb units="m/s" is_input="True">4.0</climb>
    </speed>
    <time>
      <hover units="min" is_input="True">0.0</hover>
    </time>
  </specifications>
  <data>
    <ESC>
      <efficiency is_input="True">0.95<!--ESC efficiency--></efficiency>
      <reference>
        <mass units="kg" is_input="True">0.115<!--ESC mass reference for scaling law--></mass>
        <power units="W" is_input="True">3108.0<!--ESC max. power reference for scaling law--></power>
        <voltage units="V" is_input="True">44.4<!--ESC voltage reference for scaling law--></voltage>
      </reference>
      <settings>
        <mass>
          <k is_input="True">1.0<!--Oversizing coefficient on ESC mass--></k>
        </mass>
        <power>
          <k is_input="True">1.0<!--Oversizing coefficient on ESC power--></k>
        </power>
        <voltage>
          <k is_input="True">1.0<!--Oversizing coefficient on ESC voltage--></k>
        </voltage>
      </settings>
    </ESC>
    <avionics>
      <power units="W" is_input="True">0.0<!--Avionics power consumption--></power>
    </avionics>
    <battery>
      <discharge_limit is_input="True">0.8<!--Battery discharge limit--></discharge_limit>
      <reference>
        <capacity units="A*s" is_input="True">12240.0<!--Battery capacity reference for scaling law--></capacity>
        <mass units="kg" is_input="True">0.329<!--Battery mass reference for scaling law--></mass>
        <voltage units="V" is_input="True">14.8<!--Battery voltage reference for scaling law--></voltage>
        <volume units="cm**3" is_input="True">150.93<!--Battery volume reference for scaling law--></volume>
        <current>
          <max units="A" is_input="True">170.0<!--Battery max. current reference for scaling law--></max>
        </current>
      </reference>
      <cell>
        <voltage>
          <estimated units="V" is_input="True">3.7<!--Voltage of one cell (estimated value)--></estimated>
        </voltage>
      </cell>
    </battery>
    <gearbox>
      <mass units="kg" is_input="True">0.0<!--Gearbox mass--></mass>
    </gearbox>
    <payload>
      <mass units="kg" is_input="True">0.22<!--Payload mass--></mass>
      <power units="W" is_input="True">2.0<!--Payload power consumption--></power>
    </payload>
    <mission>
      <speed>
        <forward units="m/s" is_input="True">15.0<!--Forward flight velocity--></forward>
      </speed>
    </mission>
    <motor>
      <reference>
        <coefficient units="N*m/A" is_input="True">0.03</coefficient>
        <length units="m" is_input="True">0.088<!--Motor length reference for scaling law--></length>
        <mass units="kg" is_input="True">0.575<!--Motor mass reference for scaling law--></mass>
        <resistance units="V/A" is_input="True">0.03<!--Motor resistance reference for scaling law--></resistance>
        <torque>
          <coefficient units="N*m/A" is_input="True">0.03<!--Motor torque coefficient reference for scaling law--></coefficient>
          <friction units="N*m" is_input="True">0.03<!--Motor friction torque reference for scaling law--></friction>
          <max units="N*m" is_input="True">2.81714285714<!--Motor max. torque reference for scaling law--></max>
          <nominal units="N*m" is_input="True">2.32<!--Motor nominal torque reference for scaling law--></nominal>
        </torque>
      </reference>
      <settings>
        <speed>
          <k is_input="True">1.0<!--Oversizing coefficient of motor speed--></k>
        </speed>
        <torque>
          <k is_input="True">1.0<!--Oversizing coefficient on motor torque--></k>
        </torque>
      </settings>
    </motor>
    <propeller>
      <advance_ratio>
        <climb is_input="True">0.1<!--Advance ratio (climb)--></climb>
        <forward is_input="True">0.1<!--Advance ratio (forward flight)--></forward>
      </advance_ratio>
      <reference>
        <diameter units="m" is_input="True">0.2794<!--Propeller diameter reference for scaling law--></diameter>
        <mass units="kg" is_input="True">0.014999<!--Propeller mass reference for scaling law--></mass>
        <ND>
          <max units="m/s" is_input="True">44.45<!--Max. rotational speed not to be exceeded--></max>
        </ND>
      </reference>
      <geometry>
        <beta>
          <estimated is_input="True">0.3<!--Angle pitch/diameter (estimated value)--></estimated>
        </beta>
      </geometry>
      <settings>
        <ND>
          <k is_input="True">1.0<!--Undersizing coefficient on propeller rotational speed--></k>
        </ND>
      </settings>
    </propeller>
    <structure>
      <aerodynamics>
        <Cd is_input="True">1.3<!--Drag coefficient--></Cd>
      </aerodynamics>
      <arms>
        <number is_input="True">4.0<!--Number of arms--></number>
        <prop_per_arm is_input="True">1.0<!--Number of propellers per arm (single or coaxial configuration)--></prop_per_arm>
        <material>
          <density units="kg/m**3" is_input="True">1700.0<!--Arms material density--></density>
          <stress>
            <max units="N/m**2" is_input="True">70000000.0<!--Arms material maximum allowable stress--></max>
          </stress>
        </material>
        <settings>
          <diameter>
            <k is_input="True">0.9<!--Inner to outer beam diameter ratio--></k>
          </diameter>
        </settings>
      </arms>
      <reference>
        <arms>
          <mass units="kg" is_input="True">0.14<!--Arms mass reference for scaling law--></mass>
        </arms>
        <body>
          <mass units="kg" is_input="True">0.347<!--Body mass reference for scaling law--></mass>
          <surface>
            <front units="m**2" is_input="True">0.02<!--Front surface reference for scaling law--></front>
            <top units="m**2" is_input="True">0.045<!--Top surface reference for scaling law--></top>
          </surface>
        </body>
      </reference>
    </structure>
    <system>
      <reference>
        <MTOW units="kg" is_input="True">1.4<!--Maximum Take Off Weight reference for scaling law--></MTOW>
      </reference>
      <settings>
        <MTOW>
          <k is_input="True">5.0<!--Oversizing coefficient on load mass--></k>
        </MTOW>
      </settings>
    </system>
  </data>
</FASTOAD_model>
